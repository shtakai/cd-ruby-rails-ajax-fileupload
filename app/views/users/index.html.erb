<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% if @users %>
  <% @users.each do |user| %>
    <p>name: <%= user.first_name %> <%= user.last_name %></p>
    <p>Email: <%= user.email %></p>
    <%= image_tag user.avatar.url %>
  <% end %>
<%end%>

<p><a href="/users/new">Add new</a></p>
